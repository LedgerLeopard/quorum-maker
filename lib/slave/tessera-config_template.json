{
    "type": "config",
    "useWhiteList": false,
    "disablePeerDiscovery": false,
    "jdbc": {
        "url": "jdbc:h2:file:/home/node/qdata/#nodeName#;AUTO_SERVER=TRUE",
        "autoCreateTables": false
    },
    "keys": {
        "keyData": [
            {
                "privateKeyPath": "/home/node/keys/#nodeName#.key",
                "publicKeyPath": "/home/node/keys/#nodeName#.pub"
            }
        ]
    },
    "serverConfigs": [
        {
            "app": "Q2T",
            "enabled": true,
            "communicationType": "REST",
            "serverAddress": "unix:///home/node/qdata/#nodeName#.ipc"
        },
        {
            "app": "P2P",
            "enabled": true,
            "communicationType": "REST",
            "hostName": "#localIp#",
            "port": #port#,
            "serverAddress": "http://#localIp#:#port#/",
            "sslConfig": {
                "tls": "OFF"
            }
        }
    ],
    "peer": [
        {
            "url": "http://#localIp#:#port#/"
        },
        {
            "url": "http://#masterIp#:#masterPort#/"
        }
    ],
    "alwaysSendTo": [],
    "features": {
        "enableRemoteKeyValidation": false
    },
    "encryptor": {
        "type": "NACL"
    }
}